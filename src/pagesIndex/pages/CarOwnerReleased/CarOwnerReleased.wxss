/*搜索导航*/

.header {
  width: 100%;
  line-height: 200rpx;
  background: #fff;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  -ms-flex-pack: end;
  justify-content: flex-end;
}

.header .header_left {
  width: 60%;
  height: 100%;
}

.header .header_left .location {
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  padding: 20rpx 0;
}

.header .header_left .location > input {
  width: 100%;
}

.header .header_left .location .loc_icon {
  margin-right: 30rpx;
  margin-left: -70rpx;
}

.header .header_left .location:nth-child(1) {
  border-bottom: 1rpx solid #ededf1;
}

.header .header_left .locationCenter {
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  padding: 10rpx 0;
}

.header .header_left .locationCenter > input {
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.header .header_left .locationCenter .loc_icon {
  margin-right: 30rpx;
  margin-left: -70rpx;
}

.header .header_left .location_Next .location:nth-child(1) {
  padding: 10rpx 0;
  border-bottom: 2rpx solid #ededf1;
  /*border-top: 1rpx solid #ededf1;*/
}

.header .header_left .location_Next .location:nth-child(2) {
  padding: 10rpx 0;
  border-bottom: 1rpx solid #ededf1;
}

.header .header_left .location_Next .location:nth-child(3) {
  padding: 10rpx 0;
  border-bottom: 2rpx solid #ededf1;
}

.header .header_right {
  width: 25%;
  height: 100%;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
}

.header .header_right .exchange {
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin-left: 30rpx;
}

