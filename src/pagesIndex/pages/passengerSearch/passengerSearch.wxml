<!--暂无匹配行程  -->
<view class="none_dataOne">
  <view class="none_dataTop">暂无匹配行程</view>
  <view class="none_dataTop">去发布自己的行程吧</view>
  <button bindtap="">发布行程得原力</button>
</view>
<!-- 顺风车主信息 -->
<!-- <import src="../template/travelCode/travelCode.wxml" />
