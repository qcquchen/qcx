<!-- 底部提示原力框 -->
<view class="submit">
  <view class="submit_number">
    <view class="submit_Text">
      顺路行程不合适
    </view>
    <view class="submit_Text">
      去发布行程等车主抢单吧
    </view>
  </view>
  <button bindtap="submit">发布行程得原力</button>
</view>
<!-- 关键词提示 -->
<view class='keyWord-box' hidden='{{keyShow}}'>
 <view bindtap="navTo" data-item="{{item}}" class='keyWord'  wx:for="{{resData}}" wx:key="shop">
    <view class='keyWord-img'><image src='/images/index/icon_time@3x.png'></image></view>
     <view class="weui-media-box__desc">{{item.address}}</view>
</view>
</view>


