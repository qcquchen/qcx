<!--pages/index/passengerSearch/passengerSearch.wxml-->
<!-- 起始点位置挑选 -->
<view class="header">
  <view class="header_left">
    <view class="location">
      <image class="loc_icon" src="/images/index/icon_map_star@3x.png"></image>
      <view class="locationInp">
        <input style="width: 100%" value="{{value}}" type="text" placeholder="{{address}}" bindinput='searchStart' data-id='1' placeholder-style='color:#000'/>
      </view>
    </view>
    <view class="location">
      <image class="loc_icon" src="/images/index/icon_map_end@3x.png"></image>
      <view class="locationInp locationInpTop">
        <input style="width: 100%" value="{{values}}" type="text" placeholder="请输入目的地" bindinput='searchEnd' data-id='2' placeholder-style='color:#000'/>
      </view>
    </view>
  </view>
  <view class="header_right">
    <image bindtap="" class="exchange" src="/images/index/btn_exchange@3x.png"></image>
  </view>
</view>
