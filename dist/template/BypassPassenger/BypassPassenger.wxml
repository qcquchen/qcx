<template name="BypassPassenger">
	<view class="tem-code_tipe" bindtap="click">顺路乘客行程</view>
	<view class="tem-travelcode">
	  <block wx:if="{{ aaa === 'aaa' ? false : true }}">
	    <view class="tem-code_user">
	      <view class="tem-user_left">
	        <image src="/images/index/btn_join_mini@3x.png"></image>
	        <view class="tem-user_mini">全村人的希望·女</view>
	      </view>
	      <view class="tem-user_right">
	        <image src="/images/index/btn_followed_mini.png"></image>
	      </view>
	    </view>
	  </block>
	  <view class="tem-code">
	    <view class="tem-code_address">
	      <view class="tem-code_address_list">
	        <image src="/images/index/icon_time@3x.png"></image>
	        <view class="tem-code_center">{{ aaa }}</view>
	        <view class="tem-code_status tem-code_status_color">80%顺路</view>
	      </view>
	      <view class="tem-code_address_list">
	        <image src="/images/index/icon_start@3x.png"></image>
	        <view class="tem-code_center">九龙山(地铁站)</view>
	        <view class="tem-code_status">八里庄街道</view>
	      </view>
	      <view class="tem-code_address_list">
	        <image src="/images/index/icon_end@3x.png"></image>
	        <view class="tem-code_center">中关村创业大厦</view>
	        <view class="tem-code_status">上地三街</view>
	      </view>
	      <view class="tem-code_priceOrSeats">
	      	<view class="tem-code_priceOrSeats_list">
	      		人数3
	      	</view>
	        <!-- <view class="tem-code_priceOrSeats_list">
	          <image src="/images/img_haveseat@3x.png"></image>
	          <image src="/images/img_haveseat@3x.png"></image>
	          <image src="/images/img_haveseat@3x.png"></image>
	          <image src="/images/img_haveseat@3x.png"></image>
	        </view> -->
	        <view class="tem-code_priceOrSeats_list">
	          <view class="tem-priceOrSeats_list_info" style="font-size: 26px;">16.0</view>
	          <view class="tem-priceOrSeats_list_info">元</view>
	        </view>
	      </view>
	    </view>
	    <view class="tem-code_bottom">
	      <view class="tem-code_bottom_laft">
	        <view class="tem-code_bottom_list">
	          <image src="/images/index/btn_good_mini@3x.png"></image>
	          <view>点赞</view>
	        </view>
	         <view class="tem-code_bottom_list">
	          <image src="/images/index/btn_msg_mini@3x.png"></image>
	          <view>评论</view>
	        </view>
	        <view class="tem-code_bottom_list">
	          <image src="/images/index/btn_share_mini@3x.png"></image>
	          <view class="tem-code_bottom_share">赚12.0元</view>
	        </view>
	      </view>
	      <view class="tem-code_btn">
	        <view>抢单</view>
	      </view>
	    </view>
	  </view>
	</view>
</template>
